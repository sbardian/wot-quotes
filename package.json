{
  "name": "wotQuotes",
  "version": "0.0.1",
  "description": "Serverless function that returns random WOT quotes.",
  "author": "Brian Andrews <sbardian@gmail.com>",
  "main": "functions/__main__.js",
  "private": true,
  "stdlib": {
    "build": "faaslang",
    "name": "sbardian/wotQuotes",
    "timeout": 10000,
    "personalize": {
      "keys": [],
      "user": []
    }
  },
  "scripts": {
    "cm": "git-cz",
    "lint": "eslint .",
    "test": "jest ."
  },
  "release": {
    "branch": "master"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint",
      "pre-push": "yarn lint"
    }
  },
  "dependencies": {
    "graphql-request": "1.8.2",
    "jsonwebtoken": "8.5.1"
  },
  "devDependencies": {
    "@babel/core": "7.4.5",
    "@babel/plugin-syntax-object-rest-spread": "7.2.0",
    "@babel/preset-env": "7.4.5",
    "babel-eslint": "10.0.2",
    "babel-jest": "24.8.0",
    "commitizen": "3.1.1",
    "cz-conventional-changelog": "2.1.0",
    "eslint": "5.16.0",
    "eslint-config-prettier": "5.1.0",
    "eslint-plugin-prettier": "3.1.0",
    "husky": "2.5.0",
    "jest": "24.8.0",
    "prettier": "1.18.2",
    "semantic-release": "15.13.16"
  }
}
